<p id="notice"><%= notice %></p>

<h1>Meme World</h1>

<h3><%= link_to 'Post New Meme', new_meme_path %></h3>

<table>
  <thead>
    <tr>
      <th>Img</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @memes.each do |meme| %>
      <tr>
        <td><img src="<%= meme.img %>" style="max-width: 300px"></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
